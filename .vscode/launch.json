    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {"CUDA_VISIBLE_DEVICES": "2"},
            "args": [
                "--root", "../../../share/semi_supervised/pineapples/PINEAPPLES2_5mts_nosplits", 
                "--num-classes","1",
                "--load-pretrained",
                "--timm-model", "swinv2_base_window8_256.ms_in1k",
                "--loss","mae",
                "--optim", "sgd",
                "--val-freq", "1",
                "--use-semi-split",
                "--semi-percentage", "1.0",
                "--dataset", "coco2017",
                "--batch-size","4",
                "--batch-size-val", "1",
                "--batch-size-labeled","1",
                "--batch-size-unlabeled", "2",
                "--ood-labeled-samples", "1",
                "--ood-unlabeled-samples", "2",
                "--aug-method","rand_augmentation",
                "--img-resolution", "1024",
                "--new-sample-size", "256", 
                "--numa","2",
                "--output-folder","PINEAPPLES2_5mts_nosplits",
                "--run-name","temp",
                "--seed","10",
                "--sam-model","h",
                "--ood-thresh", "0.8",
                "--ood-histogram-bins", "15",
                "--method", "sam_ood" //sam_alone sam_fewshot_single_class sam_fewshot_two_classes sam_ood
                ]
        }
    ]
